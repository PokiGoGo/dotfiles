BasedOnStyle : Google
AccessModifierOffset : -4
AllowShortFunctionsOnASingleLine : Inline
BreakBeforeBraces : Custom
BraceWrapping:
  AfterClass:      false
  AfterControlStatement: false
  AfterEnum:       false
  AfterFunction:   true
  AfterNamespace:  false
  AfterObjCDeclaration: false
  AfterStruct:     false
  AfterUnion:      false
  BeforeCatch:     false
  BeforeElse:      false
  IndentBraces:    false
ColumnLimit : 120
DerivePointerAlignment : false
IndentWidth : 4
KeepEmptyLinesAtTheStartOfBlocks : true
Standard : C++11
TabWidth : 4
UseTab : Always
